{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": { "isEnabled": true, "excludedTypes": "Request" }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Debug",
      "Host.Startup": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Warning",
      "Microsoft": "Warning",
      "Azure": "Warning",
      "Worker": "Information"
    }
  },
  "functionTimeout": "00:02:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:10",
      "visibilityTimeout": "00:01:00",
      "batchSize": 8,
      "maxDequeueCount": 5,
      "newBatchThreshold": 4
    }
  }
}